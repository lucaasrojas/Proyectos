@{
    Page.Title = "DemoBiblioteca";
    Layout = "_Layout.cshtml";
    
    Database db = Database.Open("Biblioteca");

    var listadoGeneros = db.Query("SELECT * FROM Genero");
    
    <div id="Contenedor">
    <table id="Resultado">
        <tr><th>ID</th><th>Genero</th></tr>
    
        
        @foreach (var item in listadoGeneros)
            {
                
                <tr><td>@item["GeneroID"]</td><td>@item["Nombre"]</td></tr>
            }
    </table>
    </div>
}


